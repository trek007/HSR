@{
    Layout = "~/Views/Shared/_KIPPLayout.cshtml";
    ViewBag.TitleValue = "KIPP - HSR";
    var siteRoot = ViewData["SiteRoot"];
    var hostUrl = ViewData["HostUrl"];
    var reportName = ViewData["ReportName"];
    var currentEnvironment = ViewData["CurrentEnvironment"];
}

    @if(reportName != null)
    {
        <div class="row" style="padding-top:10px;padding-bottom:10px;">
            @*@Html.AntiForgeryToken()*@
            <div class='tableauPlaceholder' style='width: 100%; height: 2036px;'>
                <object class='tableauViz' width='100%' height='2036' style='display:none;'>
                    <param name='host_url' value='@Html.Encode(@hostUrl)' />
                    <param name='site_root' value='@Html.Encode(@siteRoot)' />
                    <param name='name' value='@Html.Encode(@reportName)' />
                    @if (currentEnvironment == "Production")
                    {
                        <param name='tabs' value='yes' />
                    }
                    else
                    {
                        <param name='tabs' value='yes' />
                    }
                    <param name='toolbar' value='no' />
                    <param name='export' value='yes' />
                    <param name='refresh' value='yes' />
                    <param name='showShareOptions' value='true' />
                </object>
            </div>
        </div>
    }
    else
    {
        <div class="row text-center">
            <div class="col-md-12 text-center">
                <h1>Coming Soon</h1>
            </div> 
        </div>
    }




<script>
    $("[name='lnkSSD']").addClass('activeMenu');
    $("[name='lnkStudentEnrollments']").addClass('activeSubMenu');
</script>
